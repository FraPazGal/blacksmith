{
  "linters": {
    "js": {
      "type": "eslint",
      "include": "(\\.js$)"
    },
    "json": {
      "type": "jsonlint",
      "include": "(\\.json$)"
    },
    "templates": {
      "type": "text",
      "severity": {
        "3": "disabled",
        "5": "disabled"
      }
    },
    "merge-conflict": {
      "type": "merge-conflict"
    }
  }
}
